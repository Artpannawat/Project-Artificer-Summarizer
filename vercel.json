{
    "version": 2,
    "env": {
        "PIP_BREAK_SYSTEM_PACKAGES": "1",
        "CACHE_BUST": "1"
    },
    "outputDirectory": "dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/users/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/admin/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/auth/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/login",
            "destination": "/api/index.py"
        },
        {
            "source": "/register",
            "destination": "/api/index.py"
        },
        {
            "source": "/register_v2",
            "destination": "/api/index.py"
        },
        {
            "source": "/summarize(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/health",
            "destination": "/api/index.py"
        },
        {
            "source": "/docs",
            "destination": "/api/index.py"
        },
        {
            "source": "/openapi.json",
            "destination": "/api/index.py"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}